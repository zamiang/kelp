{
  "name": "kelp",
  "version": "0.0.1",
  "private": true,
  "description": "Kelp brings your data together in one place",
  "homepage": "https://www.kelp.nyc",
  "repository": "https://github.com/zamiang/kelp",
  "license": "SEE LICENSE IN LICENSE",
  "author": {
    "name": "Brennan Moore",
    "email": "brennanmoore@gmail.com",
    "url": "https://www.zamiang.com"
  },
  "sideEffects": false,
  "type": "module",
  "main": "pages/_index.tsx",
  "scripts": {
    "analyze": "ANALYZE=true next build",
    "audit-dependencies": "npm audit --audit-level=high --production",
    "build": "next build",
    "build-extension": "rimraf extension/dist && NODE_ENV=production webpack --config=extension/webpack.config.js",
    "build-extension:safari": "rimraf 'extension/safari/extension Extension/Resources' && NODE_ENV=production webpack --config=extension/webpack-safari.config.js",
    "css:analyze": "node analyze-css-usage.cjs",
    "css:analyze:watch": "nodemon --watch extension/src --ext css,tsx,ts,jsx,js --exec 'npm run css:analyze'",
    "css:purge:dry": "purgecss --config purgecss-analyze.config.cjs --output ./css-analysis/dry-run",
    "css:stats": "node analyze-css-usage.cjs && open css-analysis/css-analysis-report.html",
    "dev": "next -p ${PORT:-3000}",
    "dev:extension": "NODE_ENV=development webpack --watch --config=extension/webpack.dev.js",
    "format": "prettier --write \"{pages,components,constants,extension}/**/*.{ts,tsx,js}\"",
    "format:check": "prettier --list-different \"{pages,components,constants}/**/*.{ts,tsx,js}\"",
    "lint": "npx eslint './**/*.{js,ts,tsx}'",
    "lint:exports": "npx ts-unused-exports tsconfig-website-and-extension.json --exitWithCount --searchNamespaces --showLineNumber --excludePathsFromReport='pages;components/store/helpers.ts;components/store/use-store.tsx;components/shared/calendar-helpers.ts;components/store/*;store/utils/*'",
    "lint:fix": "npx eslint --fix './**/*.{js,ts,tsx}'",
    "start": "next start -p ${PORT:-3000}",
    "start-extension": "webpack --watch",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "typecheck": "node node_modules/.bin/tsc --project tsconfig.json --noEmit --pretty"
  },
  "dependencies": {
    "@azure/msal-browser": "4.20.0",
    "@azure/msal-react": "3.0.18",
    "@emotion/cache": "11.14.0",
    "@emotion/react": "11.14.0",
    "@emotion/server": "11.11.0",
    "@emotion/styled": "11.14.1",
    "@microsoft/microsoft-graph-client": "3.0.7",
    "@mui/lab": "7.0.0-beta.16",
    "@mui/material": "7.3.1",
    "@mui/styles": "6.5.0",
    "@rehooks/component-size": "1.0.3",
    "@svgr/webpack": "8.1.0",
    "@types/chrome": "0.1.3",
    "@types/express": "5.0.3",
    "@types/gapi": "0.0.47",
    "@types/gapi.auth2": "0.0.61",
    "@types/gapi.client.calendar-v3": "0.0.4",
    "@types/gapi.client.drive": "3.0.15",
    "@types/gapi.client.people": "1.0.5",
    "@types/lodash": "4.17.20",
    "@types/pluralize": "0.0.33",
    "@types/psl": "1.1.3",
    "@types/react-linkify": "1.0.4",
    "@types/react-onclickoutside": "6.7.10",
    "@vercel/analytics": "1.5.0",
    "buffer": "6.0.3",
    "clsx": "2.1.1",
    "date-fns": "4.1.0",
    "express": "5.1.0",
    "fuse.js": "7.1.0",
    "idb": "8.0.3",
    "lodash": "4.17.21",
    "material-ui-popup-state": "5.3.6",
    "next": "15.4.6",
    "next-compose-plugins": "2.2.1",
    "next-react-svg": "1.2.0",
    "next-secure-headers": "2.2.0",
    "p-ratelimit": "1.0.1",
    "pluralize": "8.0.0",
    "psl": "1.15.0",
    "react": "19.1.1",
    "react-async-hook": "4.0.0",
    "react-dom": "19.1.1",
    "react-linkify": "1.0.0-alpha",
    "react-router-dom": "7.8.0",
    "react-use": "17.6.0",
    "rimraf": "6.0.1",
    "sitemap": "8.0.0",
    "stream-browserify": "3.0.0",
    "styled-components": "6.1.19",
    "typescript": "5.9.2",
    "url": "0.11.4",
    "url-regex": "5.0.0",
    "vm-browserify": "1.1.2",
    "webpack": "5.101.1",
    "webpack-bundle-analyzer": "4.10.2",
    "webpack-cli": "6.0.1",
    "webpack-dev-server": "5.2.2"
