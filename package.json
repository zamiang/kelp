    "styled-components": "5.2.1",
    "typescript": "4.1.3"
  },
  "devDependencies": {
    "@types/node": "14.14.22",
    "@typescript-eslint/eslint-plugin": "4.14.0",
    "@typescript-eslint/eslint-plugin-tslint": "4.14.0",
    "@typescript-eslint/parser": "4.14.0",
    "eslint": "7.18.0",
    "eslint-plugin-import": "2.22.1",
