    "material-ui-popup-state": "1.7.1",
    "next": "10.0.5",
    "next-auth": "3.1.0",
    "next-secure-headers": "2.1.0",
    "pg": "8.5.1",
    "pg-connection-string": "2.4.0",
    "pluralize": "8.0.0",
    "react": "16.14.0",
    "react-async-hook": "3.6.2",
    "react-dom": "16.14.0",
    "react-hook-form": "6.14.2",
    "react-linkify": "1.0.0-alpha",
    "react-onclickoutside": "6.9.1",
    "react-popper": "2.2.4",
    "rollbar": "2.19.4",
