    "@popperjs/core": "2.9.2",
    "@rehooks/component-size": "1.0.3",
    "@slack/web-api": "6.2.3",
    "@types/chrome": "0.0.141",
    "@types/d3": "6.5.0",
    "@types/express": "4.17.11",
    "@types/faker": "5.5.5",
    "@types/gapi": "0.0.39",
    "@types/gapi.auth2": "0.0.54",
    "@types/gapi.client.calendar": "3.0.10",
    "@types/gapi.client.drive": "3.0.13",
    "@types/gapi.client.driveactivity": "2.0.0",
    "@types/gapi.client.gmail": "1.0.3",
    "@types/gapi.client.people": "1.0.3",
    "@types/gapi.client.tasks": "1.0.3",
    "@types/lodash": "4.14.169",
    "@types/pluralize": "0.0.29",
    "@types/react-beautiful-dnd": "13.0.0",
