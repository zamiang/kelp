    "eslint": "8.14.0",
    "eslint-config-next": "12.1.5",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-material-ui": "1.0.1",
    "eslint-plugin-react": "7.29.4",
    "eslint-plugin-security": "1.5.0",
    "file-loader": "6.2.0",
    "prettier": "2.6.2",
    "process": "0.11.10",
    "style-loader": "3.3.1",
    "ts-loader": "9.3.0",
    "ts-unused-exports": "7.0.3",
    "tslint": "6.1.3",
    "url-loader": "4.1.1"
  },
  "engines": {
    "node": "14.x",
    "npm": "8.x"
  },
  "cacheDirectories": [
    ".next/cache"
  ]
}
