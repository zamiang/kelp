{
  "name": "kelp",
  "version": "0.0.1",
  "private": true,
  "description": "Kelp brings your data together in one place",
  "homepage": "https://www.kelp.nyc",
  "repository": "https://github.com/zamiang/kelp",
  "license": "SEE LICENSE IN LICENSE",
  "author": {
    "name": "Brennan Moore",
    "email": "brennanmoore@gmail.com",
    "url": "https://www.zamiang.com"
  },
  "main": "pages/_index.tsx",
  "scripts": {
    "analyze": "ANALYZE=true next build",
    "audit-dependencies": "npm audit --audit-level=high",
    "build": "next build",
    "build-extension": "rimraf extension/dist && webpack --config=extension/webpack.config.js",
    "dev": "next -p ${PORT:-3000}",
    "lint": "npx eslint './**/*.{js,ts,tsx}'",
    "lint:exports": "npx ts-unused-exports tsconfig.json --exitWithCount --searchNamespaces --showLineNumber --excludePathsFromReport='pages;components/timeline/timeline.tsx;components/store/use-gapi.ts'",
    "lint:fix": "npx eslint --fix './**/*.{js,ts,tsx}'",
    "prettier": "prettier --write \"{pages,components,constants,extension}/**/*.{ts,tsx,js}\"",
    "prettier:check": "prettier --list-different \"{pages,components,constants}/**/*.{ts,tsx,js}\"",
    "start": "next start -p ${PORT:-3000}",
    "start-extension": "webpack --watch",
    "typecheck": "node node_modules/.bin/tsc --project tsconfig.json --noEmit --pretty"
  },
  "dependencies": {
    "@material-ui/core": "4.11.2",
    "@material-ui/icons": "4.11.2",
