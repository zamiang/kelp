{
  "name": "kelp",
  "version": "0.0.1",
  "private": true,
  "description": "Kelp brings your data together in one place",
  "homepage": "https://www.kelp.nyc",
  "repository": "https://github.com/zamiang/kelp",
  "license": "SEE LICENSE IN LICENSE",
  "author": {
    "name": "Brennan Moore",
    "email": "brennanmoore@gmail.com",
    "url": "https://www.zamiang.com"
  },
  "main": "pages/_index.tsx",
  "scripts": {
    "audit-dependencies": "npm audit --audit-level=high",
    "build": "next build",
    "dev": "next -p ${PORT:-3000}",
    "lint": "npx eslint './**/*.{js,ts,tsx}'",
    "lint:exports": "npx ts-unused-exports tsconfig.json --exitWithCount --searchNamespaces --showLineNumber --excludePathsFromReport='pages;components/timeline/timeline.tsx'",
    "lint:fix": "npx eslint --fix './**/*.{js,ts,tsx}'",
    "prettier": "prettier --write \"{pages,components,constants}/**/*.{ts,tsx,js}\"",
    "prettier:check": "prettier --list-different \"{pages,components,constants}/**/*.{ts,tsx,js}\"",
    "start": "next start -p ${PORT:-3000}",
    "typecheck": "node node_modules/.bin/tsc --project tsconfig.json --noEmit --pretty"
  },
  "dependencies": {
    "@material-ui/core": "4.11.3",
    "@material-ui/icons": "4.11.2",
    "@material-ui/lab": "4.0.0-alpha.57",
    "@material-ui/styles": "4.11.3",
    "@popperjs/core": "2.6.0",
    "@rehooks/component-size": "1.0.3",
    "@slack/web-api": "6.0.0",
    "@types/d3": "6.2.0",
    "@types/express": "4.17.11",
    "@types/faker": "5.1.5",
    "@types/gapi": "0.0.39",
    "@types/gapi.auth2": "0.0.52",
    "@types/gapi.client.calendar": "3.0.10",
    "@types/gapi.client.drive": "3.0.13",
    "@types/gapi.client.driveactivity": "2.0.0",
    "@types/gapi.client.gmail": "1.0.3",
    "@types/gapi.client.people": "1.0.3",
    "@types/get-urls": "9.1.3",
    "@types/lodash": "4.14.168",
    "@types/next-auth": "3.1.21",
    "@types/pluralize": "0.0.29",
    "@types/react-linkify": "1.0.0",
    "@types/react-onclickoutside": "6.7.3",
    "@types/stopword": "1.0.0",
    "babel-plugin-styled-components": "1.12.0",
    "clsx": "1.1.1",
    "d3": "6.5.0",
    "date-fns": "2.16.1",
